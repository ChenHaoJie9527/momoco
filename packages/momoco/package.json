{
  "name": "momoco",
  "description": "A momoco icon library package for web and javascript applications.",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChenHaoJie9527/momoco",
    "directory": "packages/momoco"
  },
  "keywords": [
    "momoco",
    "HTML",
    "Icons",
    "SVG",
    "Feather",
    "Loading",
    "Svg Loading",
    "Animate",
    "Transition"
  ],
  "amdName": "momoco",
  "scripts": {
    "dev": "vite build --watch",
    "build": "pnpm clean && pnpm build",
    "test": "vitest",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "version": "pnpm version --git-tag-version=false"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/momoco.umd.cjs",
  "module": "./dist/momoco.js",
  "types": "./dist/momoco.d.ts",
  "sideEffects": false,
  "typings": "./dist/momoco.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "jsdom": "^24.1.1",
    "typescript": "^5.0.4",
    "vite": "^5.x",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^2.x",
    "@momoco/core": "workspace:*"
  }
}
